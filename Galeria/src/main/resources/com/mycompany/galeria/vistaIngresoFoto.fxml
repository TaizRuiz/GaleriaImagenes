<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.galeria.VistaIngresoFotoController">
   <children>
      <Label layoutX="60.0" layoutY="109.0" text="titulo" />
      <Label layoutX="59.0" layoutY="164.0" text="fecha" />
      <Label layoutX="60.0" layoutY="229.0" text="lugar" />
      <Label layoutX="50.0" layoutY="291.0" text="personas" />
      <Button fx:id="btnGuardar" layoutX="192.0" layoutY="357.0" mnemonicParsing="false" onAction="#guardarFoto" text="Guardar" />
      <TextField fx:id="txtTitulo" layoutX="124.0" layoutY="105.0" promptText="titulo de la foto" />
      <TextField fx:id="txtFecha" layoutX="124.0" layoutY="160.0" promptText="fecha de la foto" />
      <TextField fx:id="txtLugar" layoutX="124.0" layoutY="225.0" promptText="lugar de la foto" />
      <TextField fx:id="txtPersonas" layoutX="125.0" layoutY="287.0" promptText="personas en la foto" />
      <Label layoutX="62.0" layoutY="65.0" text="ruta" />
      <TextField fx:id="txtRuta" layoutX="124.0" layoutY="61.0" promptText="ruta" />
      <Button fx:id="btnArchivo" layoutX="294.0" layoutY="61.0" mnemonicParsing="false" onAction="#cargarArchivo" text="cargar" />
   </children>
</AnchorPane>
